pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

interface IStargateRouter {
    function swap(
        uint16 _dstChainId,
        uint256 _srcPoolId,
        uint256 _dstPoolId,
        address payable _refundAddress,
        uint256 _amountLD,
        uint256 _minAmountLD,
        lzTxObj memory _lzTxParams,
        bytes calldata _to,
        bytes calldata _payload
    ) external payable;

    // Router.sol method to get the value for swap()
    function quoteLayerZeroFee(
        uint16 _dstChainId,
        uint8 _functionType,
        bytes calldata _toAddress,
        bytes calldata _transferAndCallPayload,
        lzTxObj memory _lzTxParams
    ) external view returns (uint256, uint256);

    struct lzTxObj {
        uint256 dstGasForCall;
        uint256 dstNativeAmount;
        bytes dstNativeAddr;
    }
}

contract BridgeTest {

    IStargateRouter public router;
    address public bridgeToken;

    constructor(
        address stargateRouter_,
        address token_
    ) {

        IERC20(token_).approve(stargateRouter_, type(uint256).max);
        bridgeToken = token_;

        router = IStargateRouter(stargateRouter_);
    }

    function createPayload(address destAddress, address destToken) private returns (bytes memory) {
        bytes memory data;
        {
            data = abi.encode(destAddress, destToken);
        }
        return data;
    }

    function getLzParams(address destinationAddress) private pure returns (IStargateRouter.lzTxObj memory) {
        return IStargateRouter.lzTxObj({
            dstGasForCall : 500000,
            dstNativeAmount : 0,
            dstNativeAddr : abi.encodePacked(destinationAddress)
        });
    }

    function bridge(
        uint256 amount,
        address destinationAddress,
        address destinationToken,
        address destinationContract
    ) external payable {

        IERC20(bridgeToken).transferFrom(msg.sender, address(this), amount);
        bytes memory payload = createPayload(destinationAddress, destinationToken);

        router.swap{ value: msg.value }(
            2,//destChainId, (BSC Mainnet)
            1,//srcPoolId, (EthMainnet USDC poolId)
            2,//destinationPoolId, (BscMainnet USDC PoolId 2)
            payable(msg.sender),
            amount,
            0,
            getLzParams(destinationAddress),
            abi.encodePacked(destinationContract),
            payload
        );
    }
}

