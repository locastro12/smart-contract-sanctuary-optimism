pragma solidity ^0.8.10;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import {Swapper} from "./../interfaces/Swapper.sol";
import {UniRouterV2, UniFactoryV2} from "./../interfaces/UniV2.sol";

contract UniSwapperV2 is Swapper, Ownable {

    UniRouterV2 public router;
    UniFactoryV2 public factory;

    constructor(address amm_) {
        setDex(amm_);
    }

    function setDex(address router_) public onlyOwner {
        router = UniRouterV2(router_);
        factory = UniFactoryV2(router.factory());
    }

    function swap(address tokenA, address tokenB, uint256 amount, address recipient) external payable returns (uint256) {
        if (tokenA == address(0x0)) {
            require(msg.value == amount, "UniswapperV2/amount-mismatch");
            address[] memory path = new address[](2);
            path[0] = router.WETH();
            path[1] = tokenB;

            return router.swapExactETHForTokens{ value: amount }(
                0,
                path,
                recipient,
                block.timestamp + 300
            )[1];
        } else {
            require(IERC20(tokenA).transferFrom(msg.sender, address(this), amount), "UniswapperV2/transfer-failed");
            IERC20(tokenA).approve(address(router), amount);

            if (tokenB != address(0x0)) {
                address[] memory path = new address[](2);
                path[0] = tokenA;
                path[1] = tokenB;

                return router.swapExactTokensForTokens(
                    amount,
                    0,
                    path,
                    recipient,
                    block.timestamp + 300
                )[1];
            } else {
                address[] memory path = new address[](2);
                path[0] = tokenA;
                path[1] = router.WETH();

                return router.swapExactTokensForETH(
                    amount,
                    0,
                    path,
                    recipient,
                    block.timestamp + 300
                )[1];
            }
        }
    }

    function isTokenSupported(address bridgeToken, address token) public view returns(bool) {
        if (bridgeToken == token) {
            return true;
        } else {
            address swapTokenA = bridgeToken;
            address swapTokenB = token;
            if (bridgeToken == address(0x0)) {
                swapTokenA = router.WETH();
            }
            if (token == address(0x0)) {
                swapTokenB = router.WETH();
            }
            address pool = factory.getPair(swapTokenA, swapTokenB);
            return pool != address(0);
        }
    }

    function isTokensSupported(address bridgeToken, address[] memory tokens) external view returns(bool[] memory) {
        bool[] memory results = new bool[](tokens.length);
        for (uint256 i = 0; i < tokens.length; i++) {
            results[i] = isTokenSupported(bridgeToken, tokens[i]);
        }
        return results;
    }

    function isPairsSupported(address[][] calldata tokens) external view returns(bool[] memory) {
        bool[] memory results = new bool[](tokens.length);
        for (uint256 i = 0; i < tokens.length; i++) {
            results[i] = isTokenSupported(tokens[i][0], tokens[i][1]);
        }
        return results;
    }

    // do not used on-chain, gas inefficient!
    function quote(address tokenA, address tokenB, uint256 amount) external returns (uint256) {
        if (tokenA == tokenB) {
            return amount;
        }
        address swapTokenA = tokenA;
        address swapTokenB = tokenB;
        if (tokenA == address(0x0)) {
            swapTokenA = router.WETH();
        }
        if (tokenB == address(0x0)) {
            swapTokenB = router.WETH();
        }
        address[] memory path = new address[](2);
        path[0] = swapTokenA;
        path[1] = swapTokenB;
        return router.getAmountsOut(amount, path)[1];
    }
}

