            return true;
        } else {
            address swapTokenA = bridgeToken;
            address swapTokenB = token;
            if (bridgeToken == address(0x0)) {
                swapTokenA = router.WAVAX();
            }
            if (token == address(0x0)) {
                swapTokenB = router.WAVAX();
            }
            address pool = factory.getPair(swapTokenA, swapTokenB);
            return pool != address(0);
        }
    }

    function isTokensSupported(address bridgeToken, address[] memory tokens) external view returns(bool[] memory) {
        bool[] memory results = new bool[](tokens.length);
        for (uint256 i = 0; i < tokens.length; i++) {
            results[i] = isTokenSupported(bridgeToken, tokens[i]);
        }
        return results;
    }

    function isPairsSupported(address[][] calldata tokens) external view returns(bool[] memory) {
        bool[] memory results = new bool[](tokens.length);
        for (uint256 i = 0; i < tokens.length; i++) {
            results[i] = isTokenSupported(tokens[i][0], tokens[i][1]);
        }
        return results;
    }

    // do not used on-chain, gas inefficient!
    function quote(address tokenA, address tokenB, uint256 amount) external returns (uint256) {
        if (tokenA == tokenB) {
            return amount;
        }
        address swapTokenA = tokenA;
        address swapTokenB = tokenB;
        if (tokenA == address(0x0)) {
            swapTokenA = router.WAVAX();
        }
        if (tokenB == address(0x0)) {
            swapTokenB = router.WAVAX();
        }
        address[] memory path = new address[](2);
        path[0] = swapTokenA;
        path[1] = swapTokenB;
        return router.getAmountsOut(amount, path)[1];
    }
}

