        VerificationKey memory vk = _getVerificationKey(merkleTreeDepth - 16);

        Pairing.G1Point memory vk_x = vk.IC[0];

        vk_x = Pairing.addition(vk_x, Pairing.scalar_mul(vk.IC[1], merkleTreeRoot));
