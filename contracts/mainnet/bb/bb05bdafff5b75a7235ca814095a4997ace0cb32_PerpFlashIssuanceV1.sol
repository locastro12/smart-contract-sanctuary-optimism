    ) 
        external 
        isInitializedSet(_setToken)
    {

        uint256 usdcBalanceBefore = usdc.balanceOf(address(this));

        TransferHelper.safeTransferFrom(address(usdc), msg.sender, address(this), _maxAmountIn);

        // Get component Sets required
        (
            address[] memory componentSets,
            uint256[] memory componentSetNotionalUnits
        ) = basicIssuanceModule.getRequiredComponentUnitsForIssue(_setToken, _amount);

        address[] memory checkedComponents = new address[](1);
        checkedComponents[0] = address(usdc);

        for (uint256 i = 0; i <  componentSets.length; i++) {
            uint256[] memory maxTokenAmountsIn = new uint256[](1);
            maxTokenAmountsIn[0] = _maxAmountForComponentSets[i];
        
            slippageIssuanceModule.issueWithSlippage(
                ISetToken(componentSets[i]),
                componentSetNotionalUnits[i],
                checkedComponents,
                maxTokenAmountsIn,
                address(this)
            );
        }

        // Issue parent Set to caller
        basicIssuanceModule.issue(_setToken, _amount, msg.sender);

        _collectIssuanceFeeAndReturnRest(usdcBalanceBefore, _maxAmountIn);
    }

    function redeemFixedSetForUsdc(
        ISetToken _setToken,
        uint256 _amount,
        uint256[] memory _minAmountForComponentSets,
        uint256 _minAmountOut
    )   
        external 
        isInitializedSet(_setToken) 
    {
        
        uint256 usdcBalanceBefore = usdc.balanceOf(address(this));

        TransferHelper.safeTransferFrom(address(_setToken), msg.sender, address(this), _amount);

        // Redeem parent Set to this contract
        basicIssuanceModule.redeem(_setToken, _amount, address(this));

        // Get component Sets
        (
            address[] memory componentSets,
            uint256[] memory componentSetNotionalUnits
        ) = basicIssuanceModule.getRequiredComponentUnitsForIssue(_setToken, _amount);

        address[] memory checkedComponents = new address[](1);
        checkedComponents[0] = address(usdc);

        for (uint256 i = 0; i <  componentSets.length; i++) {
            uint256[] memory minTokenAmountsOut = new uint256[](1);
            minTokenAmountsOut[0] = _minAmountForComponentSets[i];
        
            slippageIssuanceModule.redeemWithSlippage(
                ISetToken(componentSets[i]),
                componentSetNotionalUnits[i],
                checkedComponents,
                minTokenAmountsOut,
                address(this)
            );
        }

        // Collect fees
        _collectRedemptionFeeAndReturnAmount(usdcBalanceBefore, _minAmountOut);
    }

    // INTERNAL FUNCTIONS

    function _validateComponentSet(ISetToken _setToken) internal view {
        require(controller.isSet(address(_setToken)), "Invalid parent Set");
        
        address[] memory components = _setToken.getComponents();
        bool isUsdcComponent = _setToken.isComponent(address(usdc));
        require(components.length == 1 && isUsdcComponent, "Invalid component Set");
    }

    function _collectIssuanceFeeAndReturnRest(uint256 _usdcBalanceBefore, uint256 _amountIn) internal {
        uint256 usdcBalanceAfter = usdc.balanceOf(address(this));
        
        uint256 usdcUsed = _usdcBalanceBefore.add(_amountIn).sub(usdcBalanceAfter);
        uint256 usdcDust = usdcBalanceAfter.sub(_usdcBalanceBefore);
        uint256 usdcFee = usdcUsed.preciseMul(percentFee);
        
        if (usdcDust > usdcFee) {
            TransferHelper.safeTransfer(address(usdc), msg.sender, usdcDust.sub(usdcFee));
        }
    }

    function _collectRedemptionFeeAndReturnAmount(uint256 _usdcBalanceBefore, uint256 _minAmountOut) internal {        
        // Collect Fees
        uint256 usdcBalanceAfter = usdc.balanceOf(address(this));
        uint256 usdcReceived = usdcBalanceAfter.sub(_usdcBalanceBefore);
        uint256 fee = usdcReceived.preciseMul(percentFee);
        uint256 usdcAmountOut = usdcReceived.sub(fee);

        require(usdcAmountOut > _minAmountOut, "Amount out less than min");
        
        TransferHelper.safeTransfer(address(usdc), msg.sender, usdcAmountOut);
    }
}