    using PreciseUnitMath for uint256;
    using SafeMath for uint256;

    IController public immutable controller;
    ISlippageIssuanceModule public immutable slippageIssuanceModule;
    IBasicIssuanceModule public immutable basicIssuanceModule;
    IERC20 public immutable usdc;
    uint256 public percentFee;      // USDC Fee charged per issuance
    
    mapping(ISetToken => bool) public initializedSets;
    
    constructor(
        IController _controller,
        ISlippageIssuanceModule _slippageIssuanceModule,
        IBasicIssuanceModule _basicIssuanceModule,
        IERC20 _usdc,
        uint256 _percentFee
    ) {
        controller = _controller;
        slippageIssuanceModule = _slippageIssuanceModule;
        basicIssuanceModule = _basicIssuanceModule;
        usdc = _usdc;
        percentFee = _percentFee;

        // Approve USDC to SlippageIssuanceModule
        _usdc.approve(address(_slippageIssuanceModule), PreciseUnitMath.maxUint256());
    }

    // Modifier
    modifier isInitializedSet(ISetToken _setToken) {
        require(initializedSets[_setToken], "Set not initialized");
        _;
    }

    // HELPER FUNCTIONS

    function initializeSet(ISetToken _setToken) external onlyOwner {
        address[] memory componentSets = _setToken.getComponents();

        uint256 componentLength = componentSets.length;
        for(uint256 i = 0; i < componentLength; i++) {
            _validateComponentSet(ISetToken(componentSets[i]));

            // Approve SetToken component to BasicIssuanceModule
            TransferHelper.safeApprove(componentSets[i], address(basicIssuanceModule), PreciseUnitMath.maxUint256());
        }
        // Update state
        initializedSets[_setToken] = true;
    }

    // helper approve function
    function approve(address _token, address _spender, uint256 _amount) external onlyOwner {
        TransferHelper.safeApprove(_token, _spender, _amount);
    }

    function updatePercentFee(uint256 _percentFee) external onlyOwner {
        percentFee = _percentFee;
    }

    // GETTERS

    /// @dev Calculates amount of USDC required to issue a fixed amount of Set.
