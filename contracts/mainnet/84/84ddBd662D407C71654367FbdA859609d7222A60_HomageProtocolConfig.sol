pragma solidity ^0.8.12;

import {PRBMathUD60x18} from "@prb/math/contracts/PRBMathUD60x18.sol";

import "./interface/IHomageProtocolConfig.sol";
import "../lib/storage/RemintPriceConfigStorage.sol";
import "../lib/storage/HomageProtocolAddressesStorage.sol";
import "../lib/UUPSOwnableUpgradeable.sol";

contract HomageProtocolConfig is IHomageProtocolConfig, UUPSOwnableUpgradeable {
    using PRBMathUD60x18 for uint256;

    function initialize(InitializeParams calldata initializeParams)
        external
        override
        initializer
    {
        __UUPSOwnableUpgradeable_init();
        _transferOwnership(initializeParams.owner);

        _setInitialMintPrice(initializeParams.initialMintPrice);
        _setPriceInflationMultiplier(initializeParams.priceInflationMultiplier);
        _setHomageProtocolTreasury(initializeParams.homageProtocolTreasury);
        _setRemintProceedsTreasury(initializeParams.remintProceedsTreasury);
        _setL2TokenClaimBridge(initializeParams.l2TokenClaimBridge);
        _setL2EventLogger(initializeParams.l2EventLogger);
        _setL2ReplicaFactory(initializeParams.l2ReplicaFactory);
        _setL2ReplicaRegistry(initializeParams.l2ReplicaRegistry);
        _setL2ReplicaBeacon(initializeParams.l2ReplicaBeacon);
        _setL1TokenClaimBridge(initializeParams.l1TokenClaimBridge);
        _setHomageToken(initializeParams.homageToken);
        _setHomageProtocolTeam(initializeParams.homageProtocolTeam);
    }

    function setRemintProceedsTreasury(address payable remintProceedsTreasury_)
        external
        override
        onlyOwner
    {
        _setRemintProceedsTreasury(remintProceedsTreasury_);
    }

    function setHomageProtocolTreasury(address payable homageProtocolTreasury_)
        external
        override
        onlyOwner
    {
        _setHomageProtocolTreasury(homageProtocolTreasury_);
    }

    function setL2TokenClaimBridge(address l2TokenClaimBridge_)
        external
        override
        onlyOwner
    {
        _setL2TokenClaimBridge(l2TokenClaimBridge_);
    }

    function setL1TokenClaimBridge(address l1TokenClaimBridge_)
        external
        override
        onlyOwner
    {
        _setL1TokenClaimBridge(l1TokenClaimBridge_);
    }

    function setL2EventLogger(address l2EventLogger_)
        external
        override
        onlyOwner
    {
        _setL2EventLogger(l2EventLogger_);
    }

    function setL2ReplicaFactory(address l2ReplicaFactory_)
        external
        override
        onlyOwner
    {
        _setL2ReplicaFactory(l2ReplicaFactory_);
    }

    function setL2ReplicaRegistry(address l2ReplicaRegistry_)
        external
        override
        onlyOwner
    {
        _setL2ReplicaRegistry(l2ReplicaRegistry_);
    }

    function setL2ReplicaBeacon(address l2ReplicaBeacon_)
        external
        override
        onlyOwner
    {
        _setL2ReplicaBeacon(l2ReplicaBeacon_);
    }

    function setHomageToken(address homageToken_) external override onlyOwner {
        _setHomageToken(homageToken_);
    }

    function setHomageProtocolTeam(address payable homageProtocolTeam_)
        external
        override
        onlyOwner
    {
        _setHomageProtocolTeam(homageProtocolTeam_);
    }

    function setRemintPrice(uint256 remintPrice_) external override onlyOwner {
        RemintPriceConfigStorage.get().remintPrice = remintPrice_;
    }

    function remintProceedsTreasury()
        external
        view
        override
        returns (address payable)
    {
        return HomageProtocolAddressesStorage.get().remintProceedsTreasury;
    }

    function homageProtocolTreasury()
        external
        view
        override
        returns (address payable)
    {
        return HomageProtocolAddressesStorage.get().homageProtocolTreasury;
    }

    function l2TokenClaimBridge() external view override returns (address) {
        return HomageProtocolAddressesStorage.get().l2TokenClaimBridge;
    }

    function l1TokenClaimBridge() external view override returns (address) {
        return HomageProtocolAddressesStorage.get().l1TokenClaimBridge;
    }

    function l2ReplicaFactory() external view override returns (address) {
        return HomageProtocolAddressesStorage.get().l2ReplicaFactory;
    }

    function l2ReplicaRegistry() external view override returns (address) {
        return HomageProtocolAddressesStorage.get().l2ReplicaRegistry;
    }

    function l2EventLogger() external view override returns (address) {
        return HomageProtocolAddressesStorage.get().l2EventLogger;
    }

    function l2ReplicaBeacon() external view override returns (address) {
        return HomageProtocolAddressesStorage.get().l2ReplicaBeacon;
    }

    function homageToken() external view override returns (address) {
        return HomageProtocolAddressesStorage.get().homageToken;
    }

    function homageProtocolTeam()
        external
        view
        override
        returns (address payable)
    {
        return HomageProtocolAddressesStorage.get().homageProtocolTeam;
    }

    function remintPrice() external view override returns (uint256) {
        return RemintPriceConfigStorage.get().remintPrice;
    }

    function _setInitialMintPrice(uint256 newInitialMintPrice_) internal {
        require(
            newInitialMintPrice_ > 0,
            "Initial mint price must be greater than 0"
        );

        RemintPriceConfigStorage.get().initialMintPrice = newInitialMintPrice_;
    }

    function _setPriceInflationMultiplier(uint256 priceInflationMultiplier_)
        internal
    {
        require(
            priceInflationMultiplier_ != 0,
            "priceInflationMultiplier_ cannot be 0"
        );

        RemintPriceConfigStorage
            .get()
            .priceInflationMultiplier = priceInflationMultiplier_;
    }

    function _setRemintProceedsTreasury(address payable remintProceedsTreasury_)
        internal
    {
        HomageProtocolAddressesStorage
            .get()
            .remintProceedsTreasury = remintProceedsTreasury_;
    }

    function _setHomageProtocolTreasury(address payable homageProtocolTreasury_)
        internal
    {
        HomageProtocolAddressesStorage
            .get()
            .homageProtocolTreasury = homageProtocolTreasury_;
    }

    function _setL2TokenClaimBridge(address l2TokenClaimBridge_) internal {
        HomageProtocolAddressesStorage
            .get()
            .l2TokenClaimBridge = l2TokenClaimBridge_;
    }

    function _setL1TokenClaimBridge(address l1TokenClaimBridge_) internal {
        HomageProtocolAddressesStorage
            .get()
            .l1TokenClaimBridge = l1TokenClaimBridge_;
    }

    function _setL2EventLogger(address l2EventLogger_) internal {
        HomageProtocolAddressesStorage.get().l2EventLogger = l2EventLogger_;
    }

    function _setL2ReplicaFactory(address l2ReplicaFactory_) internal {
        HomageProtocolAddressesStorage
            .get()
            .l2ReplicaFactory = l2ReplicaFactory_;
    }

    function _setL2ReplicaRegistry(address l2ReplicaRegistry_) internal {
        HomageProtocolAddressesStorage
            .get()
            .l2ReplicaRegistry = l2ReplicaRegistry_;
    }

    function _setL2ReplicaBeacon(address l2ReplicaBeacon_) internal {
        HomageProtocolAddressesStorage.get().l2ReplicaBeacon = l2ReplicaBeacon_;
    }

    function _setHomageToken(address homageToken_) internal {
        HomageProtocolAddressesStorage.get().homageToken = homageToken_;
    }

    function _setHomageProtocolTeam(address payable homageProtocolTeam_)
        internal
    {
        HomageProtocolAddressesStorage
            .get()
            .homageProtocolTeam = homageProtocolTeam_;
    }
}

