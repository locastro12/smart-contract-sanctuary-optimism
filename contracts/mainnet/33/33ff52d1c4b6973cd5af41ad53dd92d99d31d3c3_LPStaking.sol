pragma solidity ^0.8.21;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/ISettings.sol";

/**
 * @title Settings Contract
 * @dev This contract allows the owner to manage the configurable parameters of the staking system. The contract includes functions to update and apply changes to the rewards rate, staking period, minimum staked amount to be eligible for rewards, and withdrawal fee for early withdrawals. The contract also includes configurable lock times to prevent users from withdrawing their stakes before a certain period of time has elapsed, and to prevent frequent changes to the contract's settings. The contract is owned by the address that deployed it and inherits from the OpenZeppelin Ownable contract. The contract also implements the ISettings interface, which defines the external functions that can be called by other contracts.
 */
contract Settings is Ownable, ISettings {
    uint256 public constant MAX_ALLOWED_TO_STAKE = 100_000_000 ether;

    /// THE LOCK TIME TO UPDATES ANY CONFIG
    uint256 public constant SETTINGS_MIN_LOCK_TIME = 0 hours;
    uint256 public constant SETTINGS_MAX_LOCK_TIME = 24 hours;
    uint256 public SETTINGS_LOCK_TIME = 1 hours;
    uint256 public NEW_SETTINGS_LOCK_TIME = 1 hours;

    /// A LOCK TIME TO CHARGE USERS THAT TRY TO WITHDRAW EARLIER THAN THE SPECIFIED LOCK TIME
    uint256 public constant WITHDRAW_EARLIER_FEE_MIN_LOCK_TIME = 24 hours;
    uint256 public constant WITHDRAW_EARLIER_FEE_MAX_LOCK_TIME = 48 hours;
    uint256 public WITHDRAW_EARLIER_FEE_LOCK_TIME = 24 hours;
    uint256 public NEW_WITHDRAW_EARLIER_FEE_LOCK_TIME = 24 hours;

    /// FEE FOR WITHDRAWING EARLIER THAN THE SPECIFIED LOCK TIME
    /// ((amount * percentage) / 10000);
    /// ((1000 * 50) / 10000); = 5 / 0.5% of 1000
    uint256 public constant WITHDRAW_EARLIER_MIN_FEE = 0; // 0%
    uint256 public constant WITHDRAW_EARLIER_MAX_FEE = 50; // 0.5%
    uint256 public WITHDRAW_EARLIER_FEE = 50; // 0.5%
    uint256 public NEW_WITHDRAW_EARLIER_FEE = 50; // 0.5%

    /// MIN VALUE STAKED TO START BEING REWARDED
    uint256 public MIN_STAKED_TO_REWARD = 0.000003 ether;
    uint256 public NEW_MIN_STAKED_TO_REWARD = 0.000003 ether;

    /// WHEN STAKING PERIOD ENDS
    uint256 public END_STAKING_UNIX_TIME;
    uint256 public NEW_END_STAKING_UNIX_TIME;

    /// TOKEN 0 REWARDS PER SECOND AVAILABLE
    uint256 public REWARDS_PER_SECOND;
    uint256 public NEW_REWARDS_PER_SECOND;

    uint256 public lastSettingsUpdate;

    event Settings_Updated();

    /**
     * @notice Allows the contract owner to update the rewards rate in tokens per day
