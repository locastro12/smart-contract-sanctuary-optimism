pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "./Mutagen.sol";

contract Mutant is ERC721Enumerable, Ownable {
    using Strings for uint256;
    uint256 private increment = 100000;

    bool public saleOpen;
    uint256 public supply = 3000;
    uint256 public publicMintCounter = 0;
    uint256 public lvl1MutationCounter = 0;
    uint256 public lvl2MutationCounter = 0;
    uint256 public lvl3MutationCounter = 0;

    IERC20 public OPToken;
    IERC721 public OAYC;
    Mutagen public mutagen1;
    Mutagen public mutagen2;
    Mutagen public mutagen3;

    // OP
    uint256 public pricePerTokenPublic = 30 ether;
    uint256 public pricePerTokenWl = 25 ether;

    // ETH
    uint256 public mutationPrice = 0.008 ether;

    uint256 public whiteListStart;
    uint256 public mutationStart;
    uint256 public publicStart;

    uint256 public maxWhitelistMint = 3;
    uint256 public maxPublicMint = 10;
    bytes32 private whiteListRoot;

    uint256 private _randomIndex;
    uint256[3000] private randomArray;

    mapping(uint256 => uint256) public mutationLvl;
    mapping(address => uint256) public whitelistMints;
    mapping(address => uint256) public publicMints;

    string public baseURI;

    enum Status {
        Closed,
        SoldOut,
        WhiteListMint,
        PublicMint,
        Mutation,
        NotStarted
    }

    struct Info {
        Status stage;
        uint256 whiteListStart;
        uint256 publicStart;
        uint256 mutationStart;
        bool saleOpen;
        uint256 supply;
        uint256 minted;
        uint256 pricePerTokenWl;
        uint256 pricePerTokenPublic;
        uint256 mutationPrice;
    }

    constructor(
        string memory name,
        string memory symbol,
        IERC20 _op,
        IERC721 _oayc
    ) ERC721(name, symbol) {
        OPToken = _op;
        OAYC = _oayc;
    }

    function info() public view returns (Info memory) {
        return Info(
            stage(),
            whiteListStart,
            publicStart,
            mutationStart,
            saleOpen,
            supply,
            totalSupply(),
            pricePerTokenWl,
            pricePerTokenPublic,
            mutationPrice
        );
    }

    function stage() public view returns (Status) {
        if (!saleOpen) {
            return Status.Closed;
        }

        uint256 ts = block.timestamp;

        if (mutationStart != 0 && ts >= mutationStart) {
            return Status.Mutation;
        } else if (publicMintCounter >= supply) {
            return Status.SoldOut;
        } else if  (publicStart != 0 && ts >= publicStart) {
            return Status.PublicMint;
        } else if (whiteListStart != 0 && ts >= whiteListStart) {
            return Status.WhiteListMint;
        }

        return Status.NotStarted;
    }

    function withdraw() public onlyOwner {
        uint balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

    function withdrawToken(IERC20 _token) public onlyOwner {
        _token.transfer(msg.sender, _token.balanceOf(address(this)));
    }

    function setSaleOpen() onlyOwner external {
        saleOpen = true;
    }

    function setSaleClose() onlyOwner external {
        saleOpen = false;
    }

    function setMaxWhitelistMint(uint256 _amount) onlyOwner external {
        maxWhitelistMint = _amount;
    }

    function setMaxPublicMint(uint256 _amount) onlyOwner external {
        maxPublicMint = _amount;
    }

    function setMutagens(Mutagen _mutagen1, Mutagen _mutagen2, Mutagen _mutagen3) onlyOwner external {
        mutagen1 = _mutagen1;
        mutagen2 = _mutagen2;
        mutagen3 = _mutagen3;
    }

    function setSaleStart(uint256 _whiteListStart, uint256 _publicStart, uint256 _mutationStart) onlyOwner external {
        whiteListStart = _whiteListStart;
        publicStart = _publicStart;
        mutationStart = _mutationStart;
    }

    function setBaseURI(string memory _uri) external onlyOwner {
        baseURI = _uri;
    }

    function setWhiteListRoot(bytes32 _merkleRoot) external onlyOwner {
        whiteListRoot = _merkleRoot;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        uint256 _tokenId = tokenId + increment * (mutationLvl[tokenId] - 1);
        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, _tokenId.toString(), ".json")) : '';
    }

    function setPricePublic(uint256 _price) external onlyOwner {
        pricePerTokenPublic = _price;
    }

    function setPriceWl(uint256 _price) external onlyOwner {
        pricePerTokenWl = _price;
    }

    function setMutationPrice(uint256 _price) external onlyOwner {
        mutationPrice = _price;
    }

    function _pickRandomUniqueId() private returns (uint256 id) {
        uint256 random = uint256(blockhash(block.number - 1));
        uint256 len = randomArray.length - _randomIndex++;
        require(len > 0, 'no ids left');
        uint256 randomIndex = random % len;
        id = randomArray[randomIndex] != 0 ? randomArray[randomIndex] : randomIndex;
        id += 1;
        randomArray[randomIndex] = uint16(randomArray[len - 1] == 0 ? len - 1 : randomArray[len - 1]);
        randomArray[len - 1] = 0;
    }

    function _pickRandomPublicId() private returns (uint256) {
        return _pickRandomUniqueId() + 10000;
    }

    function mutate1(uint256 _mutagen1, uint256 _oayc) external payable {
        require(saleOpen, "Sale is closed");
        require(address(mutagen1) != address(0), "Mutagen1 not set");
        require(address(OAYC) != address(0), "OAYC not set");
        require(mutagen1.ownerOf(_mutagen1) == msg.sender, "Caller is not owner of Mutagen 1");
        require(OAYC.ownerOf(_oayc) == msg.sender, "Caller is not owner of OAYC");
        require(msg.value >= mutationPrice, "Not enough eth attached");
        require(mutationLvl[_oayc] == 0, "Can not lvl1 mutate this OAYC");
        require(stage() == Status.Mutation, "Mutation not started");

        mutationLvl[_oayc] = 1;
        lvl1MutationCounter++;
        mutagen1.burn(_mutagen1);

        _safeMint(msg.sender, _oayc);
    }

    function mutate2(uint256 _mutagen2, uint256 _moayc) external {
        require(saleOpen, "Sale is closed");
        require(address(mutagen2) != address(0), "Mutagen2 not set");
        require(mutagen2.ownerOf(_mutagen2) == msg.sender, "Caller is not owner of Mutagen 1");
        require(ownerOf(_moayc) == msg.sender, "Caller is not owner of OAYC");
        require(mutationLvl[_moayc] == 1, "Can not lvl2 mutate this OAYC");
        require(stage() == Status.Mutation, "Mutation not started");

        mutationLvl[_moayc] = 2;
        lvl2MutationCounter++;
        lvl1MutationCounter--;
        mutagen2.burn(_mutagen2);
    }

    function mutate3(uint256 _mutagen3, uint256 _moayc) external {
        require(saleOpen, "Sale is closed");
        require(address(mutagen3) != address(0), "Mutagen3 not set");
        require(mutagen3.ownerOf(_mutagen3) == msg.sender, "Caller is not owner of Mutagen 1");
        require(ownerOf(_moayc) == msg.sender, "Caller is not owner of MOAYC");
        require(mutationLvl[_moayc] == 2, "Can not lvl3 mutate this OAYC");
        require(stage() == Status.Mutation, "Mutation not started");

        mutationLvl[_moayc] = 3;
        lvl3MutationCounter++;
        lvl2MutationCounter--;
        mutagen3.burn(_mutagen3);
    }

    function mintWhitelist(uint256 _tokenCount, bytes32[] calldata _merkleProof) external {
        require(saleOpen, "Sale is closed");
        require(address(OPToken) != address(0), "OPToken not set");
        require(stage() == Status.WhiteListMint, "Whitelist mint not started");
        require(publicMintCounter + _tokenCount <= supply, "Purchase would exceed max tokens");
        require(whitelistMints[msg.sender] + _tokenCount <= maxWhitelistMint, "Purchase would exceed max tokens");
        require(_tokenCount > 0, "Invalid token count supplied");
        require(MerkleProof.verify(
                _merkleProof,
                whiteListRoot,
                keccak256(abi.encodePacked(msg.sender)
                )), "Address is not whitelisted");

        OPToken.transferFrom(msg.sender, address(this), pricePerTokenWl * _tokenCount);

        publicMintCounter += _tokenCount;
        whitelistMints[msg.sender] += _tokenCount;

        for (uint256 i = 0; i < _tokenCount; i++) {
            uint256 _tokenId = _pickRandomPublicId();
            mutationLvl[_tokenId] = 1;
            _safeMint(msg.sender, _tokenId);
        }
    }

    function mintPublic(uint256 _tokenCount) external {
        require(saleOpen, "Sale is closed");
        require(address(OPToken) != address(0), "OPToken not set");
        require(stage() == Status.PublicMint, "Public mint not started");
        require(publicMintCounter + _tokenCount <= supply, "Purchase would exceed max tokens");
        require(publicMints[msg.sender] + _tokenCount <= maxPublicMint, "Purchase would exceed max tokens");
        require(_tokenCount > 0, "Invalid token count supplied");

        OPToken.transferFrom(msg.sender, address(this), pricePerTokenPublic * _tokenCount);
        publicMintCounter += _tokenCount;
        publicMints[msg.sender] += _tokenCount;

        for (uint256 i = 0; i < _tokenCount; i++) {
            uint256 _tokenId = _pickRandomPublicId();
            mutationLvl[_tokenId] = 1;
            _safeMint(msg.sender, _tokenId);
        }
    }

    function claim(address[] calldata _addresses, uint256 _tokenCount) external onlyOwner {
        require(publicMintCounter + _tokenCount <= supply, "Purchase would exceed max tokens");
        require(_tokenCount > 0 && _tokenCount <= 10, "Invalid token count supplied");
        publicMintCounter += _tokenCount;
        for (uint256 i = 0; i < _addresses.length; i++) {
            for (uint256 j = 0; j < _tokenCount; j++) {
                uint256 _tokenId = _pickRandomPublicId();
                mutationLvl[_tokenId] = 1;
                _safeMint(_addresses[i], _tokenId);
            }
        }
    }
}

